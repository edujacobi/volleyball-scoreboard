{
  "expo": {
    "name": "Simple Volleyball Scoreboard",
    "slug": "simple-volley-board",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0082FA"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.simplevolleyboard"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0082FA"
      },
      "package": "com.anonymous.simplevolleyboard"
    },
    "androidNavigationBar": {
      "visible": "sticky-immersive",
      "barStyle": "dark-content"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/SpaceGrotesk.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d93e7bab-b640-4e56-8944-a0f140f63d67"
      }
    }
  }
}
